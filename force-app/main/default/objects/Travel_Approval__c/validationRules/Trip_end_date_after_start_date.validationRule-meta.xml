<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Trip_end_date_after_start_date</fullName>
    <active>true</active>
    <errorConditionFormula>Trip_End_Date__c &lt; Trip_Start_Date__c</errorConditionFormula>
    <errorDisplayField>Trip_End_Date__c</errorDisplayField>
    <errorMessage>Trip end date must be greater than or equal to start date</errorMessage>
</ValidationRule>
